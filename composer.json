{
  "name": "tourze/doctrine-resolve-target-entity-bundle",
  "description": "Symfony bundle for entity decoupling using Doctrine's ResolveTargetEntity",
  "license": "MIT",
  "require": {
    "doctrine/doctrine-bundle": "^2.13",
    "doctrine/orm": "^3.0",
    "nette/php-generator": "^4.1",
    "symfony/config": "^7.3",
    "symfony/dependency-injection": "^7.3",
    "symfony/doctrine-bridge": "^7.3",
    "symfony/framework-bundle": "^7.3",
    "symfony/http-kernel": "^7.3",
    "symfony/property-access": "^7.3",
    "symfony/yaml": "^7.3",
    "tourze/bundle-dependency": "1.*",
    "tourze/symfony-dependency-service-loader": "1.0.*"
  },
  "require-dev": {
    "phpstan/phpstan": "^2.1",
    "phpunit/phpunit": "^11.5",
    "tourze/phpunit-base": "1.*",
    "tourze/phpunit-symfony-unit-test": "1.*"
  },
  "autoload": {
    "psr-4": {
      "Tourze\\DoctrineResolveTargetEntityBundle\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tourze\\DoctrineResolveTargetEntityBundle\\Tests\\": "tests"
    }
  }
}